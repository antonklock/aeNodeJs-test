{
  "name": "is-explicit",
  "version": "1.4.1",
  "description": "Combines instance of operator and typeof operator in a way that works seamlessly with objects and literals.",
  "main": "lib",
  "scripts": {
    "test": "node_modules/.bin/mocha --require babel-register --require babel-polyfill 'test/*test.js'",
    "test:watch": "node_modules/.bin/mocha --require babel-register --watch --require babel-polyfill 'test/*test.js'",
    "build": "rm -rf lib; mkdir lib; node_modules/.bin/babel src --out-dir lib --plugins transform-runtime",
    "build:watch": "npm run build -- --watch",
    "prepublishOnly": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BenZed/is-explicit"
  },
  "keywords": [
    "is",
    "operator",
    "explicit",
    "type-check",
    "type"
  ],
  "author": "Ben Gaumond",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BenZed/is-explicit/issues"
  },
  "homepage": "https://github.com/BenZed/is-explicit#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "eslint": "^4.1.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.4.2"
  }
}
